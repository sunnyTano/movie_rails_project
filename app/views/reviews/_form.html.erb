<%= form_for @review do |f| %>

    <%= f.label :movie_list %>
    <%= f.collection_select(:movie_id, Movie.all, :id, :title) %></br></br>

    <%= f.label :users %>
    <%= f.collection_select(:user_id, User.all, :id, :first_name) %></br></br>

    <%= f.label :review %>
    <%= f.text_field :content %>

    <%= f.submit %>

<% end %>


  
    